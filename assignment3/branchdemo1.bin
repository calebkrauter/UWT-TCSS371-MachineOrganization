; A conditional code example

; if R1 >= R2
; then R2 <- R1

; STORE address x3000 as the start point for the program in memory
0011 000 000000000      ; STORE address x3000

; clear some registers
0101 000 000 1 00000        ; AND (R0 <- R0 AND 0)            
0101 001 001 1 00000        ; AND (R1 <- R1 AND 0)            
0101 010 010 1 00000        ; AND (R2 <- R2 AND 0)            
0101 011 011 1 00000        ; AND (R3 <- R3 AND 0)            


; put values in R1 and R2
0001 001 001 1 00111        ; R1 <- 7
0001 010 011 1 11101        ; R1 <- -3

; take 2's complement of R2
1001 000 010 111111 ; flip the bits
0001 000 000 1 00001 ; add 1

; R1 - R2 (R1 + R0)
0001 000 000 0 00 001


; Check if R1 - R2 is negative
0000 100 0 0000 0010 ; BRANCH jump passed the assignment if R1 < R2
0001 011 011 0 00 001 ; R3 <- R1
0001 011 011 0 00 010 ; R3 <- R2
0000 111 0 0000 0001 ; jump passed the next line of code

1111 0000 0010 0101     ; halt the program using TRAP with vector x25
